<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajan.woman_safety">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
    <activity
        android:name="com.rajan.woman_safety.MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.rajan.woman_safety.Register"
        android:label="@string/title_activity_register"
        android:parentActivityName="com.rajan.woman_safety.MainActivity" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rajan.woman_safety.MainActivity" />
    </activity>
    <activity
        android:name="com.rajan.woman_safety.Display"
        android:label="@string/title_activity_display"
        android:parentActivityName="com.rajan.woman_safety.MainActivity" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rajan.woman_safety.MainActivity" />
    </activity>
    <activity
        android:name="com.rajan.woman_safety.Instructions"
        android:label="@string/title_activity_instructions"
        android:parentActivityName="com.rajan.woman_safety.MainActivity" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rajan.woman_safety.MainActivity" />
    </activity>
    <activity
        android:name="com.rajan.woman_safety.Verify"
        android:label="@string/title_activity_verify"
        android:parentActivityName="com.rajan.woman_safety.MainActivity" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rajan.woman_safety.MainActivity" />
    </activity>
        <activity
            android:name="com.rajan.woman_safety.BgService"
            android:label="GET LOCATION AND SEND SMS"
            android:parentActivityName="com.rajan.woman_safety.Display">
        </activity>
    </application>
</manifest>
